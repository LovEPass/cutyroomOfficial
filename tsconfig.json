{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
  "compilerOptions": {
    "jsx": "preserve",
    "target": "ES2017",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "lib":["ES2022","DOM","DOM.Iterable","WebWorker","WebWorker.ImportScripts"],
  },

  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}
